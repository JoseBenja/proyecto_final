<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuController">
   <children>
      <MenuButton fx:id="menubtnDispositivos" layoutY="2.0" mnemonicParsing="false" text="Dispositivos">
        <items>
          <MenuItem fx:id="itemGestionarDipositivos" mnemonicParsing="true" onAction="#clickGestionarDispositivo" text="Gestionar Dispositivos" />
        </items>
      </MenuButton>
      <MenuButton layoutX="96.0" layoutY="2.0" mnemonicParsing="false" text="Acciones">
        <items>
          <MenuItem fx:id="btnActionDevice" mnemonicParsing="false" onAction="#clickActionDevice" text="Acciones con dispositivos" />
          <MenuItem fx:id="btnActionExternDevice" mnemonicParsing="false" onAction="#clickActionExternDevice" text="Acciones externas con dispositivos" />
        </items>
      </MenuButton>
      <MenuButton layoutX="175.0" layoutY="2.0" mnemonicParsing="false" text="Reportes">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </MenuButton>
      <MenuButton layoutX="255.0" layoutY="2.0" mnemonicParsing="false" text="Usuarios">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
